    "nanoid": "^3.1.30",
    "next": "12.0.7",
    "next-cookies": "^2.0.3",
    "postcss": "^8.3.11",
    "postcss-combine-duplicated-selectors": "^10.0.3",
    "postcss-combine-media-query": "^1.0.1",
    "postcss-import": "^14.0.2",
    "postcss-nested": "^5.0.6",
    "querystring": "^0.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-feather": "^2.0.9",
    "react-intl": "^5.21.0",
    "styled-components": "^5.3.3",
